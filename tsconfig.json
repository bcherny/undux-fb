{
  "compilerOptions": {
    "baseUrl": ".",
    "declaration": false,
    "forceConsistentCasingInFileNames": true,
    "lib": [
      "dom",
      "es2015"
    ],
    "module": "esnext",
    "moduleResolution": "node",
    "noImplicitAny": true,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "preserveConstEnums": true,
    "outDir": ".",
    "paths": {
      "typed-rx-emitter": ["node_modules/typed-rx-emitter/dist.esnext"],
      "undux": ["node_modules/undux/dist.esnext"]
    },
    "skipLibCheck": true,
    "sourceMap": true,
    "strictNullChecks": true,
    "strict": true,
    "target": "esnext"
  },
  "files": [
    "./index.ts",
    "./test.ts"
  ]
}